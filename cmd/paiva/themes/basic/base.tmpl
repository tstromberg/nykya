{{ define "base" }}
    <!DOCTYPE html>
    <html lang="en">

    <head>
        <title>{{ .Title }}</title>
        {{ block "headers" .}} {{ end }}
        {{ block "style" .}} {{ end }}
        
    </head>
    <body>
        <main>
            {{template "content" .}}
        </main>

        <footer>
            {{block "footer" .}} {{end}}
        </footer>
        
        {{block "js" .}} {{end}}
    </body>
    </html>
{{ end }}